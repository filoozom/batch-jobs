    "precommit": "lint-staged"
  },
  "keywords": [
    "batch",
    "concurrency"
  ],
  "author": {
    "name": "Philippe Schommers",
    "email": "philippe@schommers.be"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filoozom/batch-jobs"
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ]
  }
}

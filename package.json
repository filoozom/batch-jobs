{
  "name": "batch-jobs",
  "version": "1.0.2",
  "description": "Batch jobs in NodeJS",
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-prettier": "^2.5.0",
    "husky": "^0.14.3",
    "prettier": "^1.10.2"
  },
  "scripts": {
